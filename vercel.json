{
  "version": 2,
  "builds": [
    {
      "src": "app/build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/build/web/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot))",
      "dest": "/app/build/web/$1"
    }
  ],
  "outputDirectory": "app/build/web"
}

